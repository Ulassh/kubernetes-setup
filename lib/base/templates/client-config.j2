---
apiVersion: v1
clusters:
- cluster:
    certificate-authority: ca.pem
    server: https://{{k8s.apiserver_lb}}:{{k8s.apiserver_port}}
  name: mycluster
contexts:
- context:
    cluster: mycluster
    user: admin
  name: mycontext
current-context: mycontext
kind: Config
preferences: {}
users:
- name: admin
  user:
    client-certificate: admin.pem
    client-key: admin-key.pem
